generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"  
  url      = env("DATABASE_URL")  
}

model Articles {
  id           Int     @id @default(autoincrement())  
  name_article String  
  price        Decimal @db.Decimal(10, 2)  
  active       Boolean @default(true)
  imageUrl     String?  

  @@map("articles")  
}
